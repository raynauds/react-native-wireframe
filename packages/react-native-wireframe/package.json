{
  "name": "react-native-wireframe",
  "version": "0.0.3",
  "description": "A pure React Native components library.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint .",
    "typecheck": "npx tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "copy-assets": "cd src && find . -name '*.png' -print0 | xargs -0 -I {} rsync -R {} ../dist/",
    "build": "npx tsc && npm run copy-assets"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "expo-blur": "~14.1.5",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-symbols": "~0.4.5",
    "react": "19.0.0",
    "react-native": "0.79.5"
  },
  "peerDependencies": {
    "@react-native-community/datetimepicker": "*",
    "@react-native-community/slider": "*",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  }
}
